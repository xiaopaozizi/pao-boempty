<!--index.wxml-->
<view class="self">
    <view class="my-list" wx:for="{{lists}}" wx:for-item="list">
        <text class="{{list.icon}}"></text>
        <navigator class="text" url="{{list.url}}">{{list.text}}</navigator>
    </view>
    <button class="logout" size="default" type="primary" bindtap="logout">退出</button>
</view>