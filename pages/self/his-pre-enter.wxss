.his-pre-enter{
  padding:0;
  width : 100%;
  height : 800px;
  background : #f5f5f5;
  font-family: '黑体';
}
.his-pre-enter .list{
  width : 100%;
  height : 100px;
  margin:10px;
  background : #fff;
  display: flex;
}
.his-pre-enter .list image {
  width : 280px;
  height:80px;
  margin:10px 0;
}
.his-pre-enter .list .info{
  margin:10px 20px;
  display: flex;
  width : 100%;
  flex-direction: column;
  align-items: flex-start;
  font-size : 14px;
}
.his-pre-enter .list .info .line {
  width : 100%;
  flex-grow: 1;
}
.his-pre-enter .list .info .line.first {
  display: flex;
  justify-content: space-between;
}
.his-pre-enter .list .info .line .status{
  padding:7px 10px 0 10px;
  border-radius: 3px;
  color : #fff;
  vertical-align: middle;
}
.his-pre-enter .list .info .line .status.doing{
  background : #180ee9;
}
.his-pre-enter .list .info .line .status.success{
  background : #0dae27;
}
.his-pre-enter .list .info .line .status.void{
  background : #e91010;
}
.his-pre-enter .list .info .line.time {
  text-align : right;
  color : #999;
  font-size:12px;
}

