@import "../template/list2.wxss";


.his-list{
  width : 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  background:#f5f5f5;
  height:auto;
}
.his-list .title {
  width : 100%;
  background:#fff;
  padding-top:20px;
  padding-bottom:10px;
  display: flex;
  flex-direction: column;
  align-content: center;
}
.his-list .title text{
  width:100%;
  color : #fe4800;
  text-align: center;
  font-weight: bold;
}
.his-list .title text.num{
  font-size:40px;
}
.his-list .title text.text{
  font-size:12px;
}
