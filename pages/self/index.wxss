.self{
  width : 100%;
  padding:20px;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-family: '黑体';
}
.self .my-list{
  width : 100%;
  margin-bottom:20px;
  display: flex;
}
.self .my-list text {
  width : 20px;
  height : 20px;
  padding:3px;
}
.self .my-list .text {
  width :70%;
  height : 20px;
  line-height : 20px;
  margin-left : 20px;
}
.self .my-list .arrow {
  width : 30px;
  height : 30px;
  float : right;
}
.self .logout{
  width : 90%;
  background:#fe9900;
  margin: 40px 0;
}