<!--index.wxml-->
<import src="../template/list.wxml" />
<view class="his-list">
    <view class="nav">
        <view class="pos pos-start">
            <image src="../../images/pos-start.png"></image>
            <text>选择提箱地</text>
        </view>
        <view class="pos pos-end">
            <image src="../../images/pos-end.png"></image>
            <text>选择目的地</text>
        </view>
    </view>
    <block wx:for="{{lists}}" wx:for-item="list">
        <template is="list" data="{{...list}}"></template>
    </block>
</view>

        
           