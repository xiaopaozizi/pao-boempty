.upload-pre-enter{
  width:100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  padding:10px;
  box-sizing: border-box;
  font-family: '黑体';
}
.upload-pre-enter .pic-title{
  color:#5e5e5e;
  font-size:14px;
  margin-bottom:10px;
}
.upload-pre-enter .pic-upload{
  width:165px;
  height:100px;
  overflow: hidden;
  margin-bottom:10px;
}
.upload-pre-enter .pic-upload image{
  width:100%;
  height:100%;
  background-size: cover;
}
.upload-pre-enter .pic-content{
  width:100%;
  height: 100vh;
  background:#f5f5f5;
  color:#666;
  font-size:12px;
}
.upload-pre-enter .pic-content label{
  color:red;
}
.upload-pre-enter .pic-content button{
  margin-top:200px;
  background:#f90;
  color:#fff;
  width:90%;
}